<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<remote name="pixel-aosp-16" fetch="https://github.com/pixel-aosp-16/" revision="refs/heads/16" />

<!-- Override these AOSP repos with ours to do minor build fixes or tweaks -->
<remove-project name="platform/frameworks/base" />
<remove-project name="platform/build" />
<remove-project name="platform/build/soong" />

<project path="frameworks/base"     name="platform_frameworks_base" remote="pixel-aosp-16" />
<project path="build/make"          name="platform_build"           remote="pixel-aosp-16">
  <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
  <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
  <linkfile src="core" dest="build/core" />
  <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
  <linkfile src="target" dest="build/target" />
  <linkfile src="tools" dest="build/tools" />
</project>
<project path="build/soong"         name="platform_build_soong"     remote="pixel-aosp-16">
  <linkfile src="root.bp" dest="Android.bp" />
  <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
</project>

<!-- Repos we want to gut like a fish -->
<remove-project name="platform/hardware/google/pixel" />

<project path="device/google/gs201"               name="device_google_gs201"                      remote="pixel-aosp-16"    revision="refs/heads/lynx" />
<project path="device/google/gs201-sepolicy"      name="device_google_gs201-sepolicy"             remote="pixel-aosp-16" />
<project path="device/google/gs-common"           name="device_google_gs-common"                  remote="pixel-aosp-16"    revision="refs/heads/port-to-16-qpr0-device" />
<project path="device/google/lynx"                name="device_google_lynx"                       remote="pixel-aosp-16"    revision="refs/heads/port-to-16-qpr0-device" />
<project path="device/google/lynx-sepolicy"       name="device_google_lynx-sepolicy"              remote="pixel-aosp-16" />
<project path="device/google/lynx-kernels/6.1"    name="device/google/lynx-kernels/6.1"           remote="aosp"             revision="refs/tags/android-15.0.0_r36" clone-depth="1" />
<project path="hardware/google/gchips"            name="platform_hardware_google_gchips"          remote="pixel-aosp-16" />
<project path="hardware/google/graphics/common"   name="platform_hardware_google_graphics_common" remote="pixel-aosp-16" />
<project path="hardware/google/graphics/gs101"    name="platform_hardware_google_graphics_gs101"  remote="pixel-aosp-16" />
<project path="hardware/google/graphics/gs201"    name="platform_hardware_google_graphics_gs201"  remote="pixel-aosp-16" />
<project path="hardware/google/pixel"             name="platform_hardware_google_pixel"           remote="pixel-aosp-16"    revision="refs/heads/lynx" />

<!-- New repos  -->
<project path="platform/packages/apps/CarrierConfig2" name="platform_packages_apps_CarrierConfig2" remote="pixel-aosp-16" />
<project path="vendor/adevtool"                       name="adevtool"                              remote="pixel-aosp-16" />
<project path="vendor/state"                          name="vendor_state"                          remote="pixel-aosp-16" clone-depth="1" />

</manifest>
